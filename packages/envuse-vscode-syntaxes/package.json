{
  "name": "envuse-language",
  "displayName": "Envuse Language support",
  "publisher": "jondotsoy",
  "license": "MIT",
  "description": "Module to load environment variables from a `.envuse` file into the `process.env`.",
  "version": "0.1.1",
  "engines": {
    "vscode": "^1.22.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [
      {
        "id": "envuse",
        "aliases": [
          "Envuse Syntax",
          "envuse"
        ],
        "extensions": [
          ".envuse"
        ]
      }
    ],
    "grammars": [
      {
        "language": "envuse",
        "scopeName": "source.envuse",
        "path": "./syntaxes/envuse.tmLanguage.json"
      }
    ]
  },
  "repository": {
    "directory": "./packages/envuse-vscode-syntaxes",
    "type": "git",
    "url": "https://github.com/JonDotsoy/envuse.git"
  }
}
