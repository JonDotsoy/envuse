{
  "name": "envuse",
  "version": "2.4.1",
  "description": "Load environment variables from a `.envuse` file into the `process.env`",
  "main": "envuse.js",
  "publishConfig": {
    "access": "public",
    "tag": "beta"
  },
  "scripts": {
    "release": "standard-version -t envuse@v --release-as patch",
    "clean": "tsc --build --clean",
    "build": "tsc",
    "build:watch": "tsc --watch",
    "dev": "tsc -w",
    "prepare": "tsc && node ./scripts/initialize_envuse-definition_file.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "git-tag-current": "git tag $npm_package_name@v$npm_package_version"
  },
  "keywords": [
    "client",
    "env",
    "environment"
  ],
  "author": "Jonathan Delgado <hi@jon.soy> (https://jon.soy)",
  "license": "ISC",
  "devDependencies": {
    "@types/jest": "^26.0.23",
    "@types/node": "^12.7.5",
    "@types/yaml": "^1.9.7",
    "jest": "^27.0.6",
    "standard-version": "^9.3.1",
    "ts-jest": "^27.0.3",
    "ts-node": "^10.0.0",
    "typescript": "^3.6.3",
    "yaml": "^1.10.2"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/JonDotsoy/envuse.git",
    "directory": "packages/envuse"
  },
  "bugs": {
    "url": "https://github.com/JonDotsoy/envuse/issues"
  },
  "homepage": "https://github.com/JonDotsoy/envuse#readme"
}
