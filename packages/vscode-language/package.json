{
  "name": "envuse-language",
  "icon": "assets/VSC-128.png",
  "displayName": "Envuse Language Support",
  "publisher": "envuse",
  "license": "MIT",
  "description": "Module to load environment variables from a `.envuse` file into the `process.env`.",
  "version": "0.1.20",
  "scripts": {
    "release": "standard-version -t vscode-language@v --release-as patch && vsce publish",
    "git-tag-current": "git tag vsce-$npm_package_name@$npm_package_version"
  },
  "engines": {
    "vscode": "^1.22.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "iconThemes": [
      {
        "id": "envuse",
        "label": "Envuse",
        "path": "./fileicons/envuse-icon-theme.json"
      }
    ],
    "languages": [
      {
        "id": "envuse",
        "aliases": [
          "Envuse Syntax",
          "envuse"
        ],
        "extensions": [
          ".envuse",
          ".envuse-lock"
        ],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "envuse",
        "scopeName": "source.envuse",
        "path": "./syntaxes/envuse.tmLanguage.json"
      }
    ]
  },
  "repository": {
    "directory": "./packages/vscode-language",
    "type": "git",
    "url": "https://github.com/JonDotsoy/envuse.git"
  },
  "devDependencies": {
    "standard-version": "^9.3.1"
  }
}
